[user]
	name = Giedrius Kudelis
----- gitconfig_email
[core]
	autocrlf = input
	safecrlf = false
----- gitconfig_editor
[alias]
    co = checkout
    ci = commit
    ce = commit --allow-empty
    dc = diff --cached
    st = status
    br = branch
    rv = revert
    pl = pull
    ps = push
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short --all
    type = cat-file -t
    dump = cat-file -p
[push]
	default = current
[pull]
	rebase = true
